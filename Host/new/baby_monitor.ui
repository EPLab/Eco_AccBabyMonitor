<?xml version="1.0" ?>
<interface>
  <object class="tk.Frame" id="main_window">
    <layout>
      <property name="column">5</property>
      <property name="padx">2</property>
      <property name="pady">2</property>
      <property name="propagate">True</property>
      <property name="row">4</property>
      <property name="sticky">nesw</property>
    </layout>
    <child>
      <object class="tk.Button" id="Button_Start">
        <property name="text" translatable="yes">Start</property>
        <property name="command">on_button_start_clicked</property>
        <property name="textvariable">current_start_text</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">nesw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="Button_Save">
        <property name="text" translatable="yes">Save</property>
        <property name="command">on_button_save_clicked</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">nesw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="Label_Freq">
        <property name="text" translatable="yes">Frequency</property>
        <layout>
          <property name="column">2</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">nesw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Menubutton" id="Menubutton_Freq">
        <property name="justify">center</property>
        <property name="text" translatable="yes">50Hz</property>
        <property name="textvariable">current_freq_text</property>
        <layout>
          <property name="column">3</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">nesw</property>
        </layout>
        <child>
          <object class="tk.Menu" id="Menu_Freq">
            <child>
              <object class="tk.Menuitem.Command" id="Command_Freq_20Hz">
                <property name="command">on_20Hz_clicked</property>
                <property name="command_id_arg">False</property>
                <property name="label" translatable="yes">20Hz</property>
              </object>
            </child>
            <child>
              <object class="tk.Menuitem.Command" id="Command_Freq_50Hz">
                <property name="command">on_50Hz_clicked</property>
                <property name="command_id_arg">False</property>
                <property name="label" translatable="yes">50Hz</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="Label_sensor_1">
        <property name="background">#ff0000</property>
        <property name="text" translatable="yes">Sensor 1 (OFF)</property>
        <property name="textvariable">label_sensor_1_text</property>
        <layout>
          <property name="column">0</property>
          <property name="ipadx">28</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">ns</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="Sensor_1">
        <property name="height">300</property>
        <property name="width">360</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">1</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="Label_sensor_2">
        <property name="background">#00ff00</property>
        <property name="text" translatable="yes">Sensor 2 (OFF)</property>
        <property name="textvariable">label_sensor_2_text</property>
        <layout>
          <property name="column">1</property>
          <property name="ipadx">28</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">ns</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="Sensor_2">
        <property name="height">300</property>
        <property name="width">360</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="Label_sensor_3">
        <property name="background">#ff0000</property>
        <property name="text" translatable="yes">Sensor 3 (OFF)</property>
        <property name="textvariable">label_sensor_3_text</property>
        <layout>
          <property name="column">2</property>
          <property name="ipadx">28</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">ns</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="Sensor_3">
        <property name="height">300</property>
        <property name="width">360</property>
        <layout>
          <property name="column">2</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="Label_sensor_4">
        <property name="background">#00ff00</property>
        <property name="text" translatable="yes">Sensor 4 (OFF)</property>
        <property name="textvariable">label_sensor_4_text</property>
        <layout>
          <property name="column">3</property>
          <property name="ipadx">28</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">ns</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="Sensor_4">
        <property name="height">300</property>
        <property name="width">360</property>
        <layout>
          <property name="column">3</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="Label_sensor_5">
        <property name="background">#00ff00</property>
        <property name="text" translatable="yes">Sensor 5 (OFF)</property>
        <property name="textvariable">label_sensor_5_text</property>
        <layout>
          <property name="column">4</property>
          <property name="ipadx">28</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">ns</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="Sensor_5">
        <property name="height">300</property>
        <property name="width">360</property>
        <layout>
          <property name="column">4</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="Button_Quit">
        <property name="command">on_button_quit_clicked</property>
        <property name="text" translatable="yes">Quit</property>
        <layout>
          <property name="column">4</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">nesw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="Label_status">
        <property name="background">#cccccc</property>
        <property name="borderwidth">1</property>
        <property name="justify">left</property>
        <property name="text" translatable="yes">length = 56 char</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">3</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property name="sticky">nesw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="Button_Rewind">
        <property name="text" translatable="yes">Rewind</property>
        <layout>
          <property name="column">3</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property name="sticky">nesw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="Button_Foward">
        <property name="text" translatable="yes">Foward</property>
        <layout>
          <property name="column">4</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property name="sticky">nesw</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
